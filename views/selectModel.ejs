<!DOCTYPE html>
<html>
    <head>
        <title>Choose a model</title>
        <link
            href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css"
            rel="stylesheet"
            id="bootstrap-css"
        />
        <!-- <link rel='stylesheet' href='/stylesheets/style.css'/>
            <link rel="stylesheet" href="/stylesheets/viewexperiment.css"> -->
        <link href="/css/style.css" rel="stylesheet" type="text/css">
    </head>

  <body>
    <div class="container">
        <!-- Navigation Bar -->
        <div class="container">
            <nav class="navbar navbar-default navbar-fixed-top">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="/home">Home </a>
                    </div> 
                    <div class="collapse navbar-collapse" id="myNavbar">
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                <form class="form logout-link" id="form" method="POST" action="/logout">
                                    <a href="/logout"
                                        onclick="document.getElementById('form').submit(); return false;"></span>
                                        Logout</a> 
                                </form>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
        <br>
        <br>
        <br>
        <br>
        <!-- content -->
        <div class="content-container row divtop">
            <!-- progress bar -->
            <div class="progress-container">
                <div class="progress-track"></div>
                <div class="progress-track-completed step1"></div>
                <div class="complete-bullet"></div>
                <div class="complete-text">
                    <span class="step-text">Complete</span>
                </div>
                <div id="step1" class="progress-step-complete">
                    <span class="step-text">Step One</span>
                </div>
                <div id="step2" class="progress-step">
                    <span class="step-text">Step Two</span>
                </div>
                <div id="step3" class="progress-step">
                    <span class="step-text">Step Three</span>
                </div>
                <div id="step4" class="progress-step">
                    <span class="step-text">Step Four</span>
                </div>
            </div>
            <h1 class="step-title">1. Choose a model type </h1>
            <div class="step-container">
                <h4 class="step-directions">Select a model type below.</h4>
                <div class="model-type-button-container">
                    <!-- <label for="VGG16">VGG16</label><br> -->
                    <button class="model-type-button" type="button" name="VGG16">
                        <p class="model-type-button-name">
                            VGG16
                        </p>
                        <p class="model-type-button-description">
                            Deep Convolutional Networks for Large-Scale Image Recognition
                        </p>
                    </button>
                    <br>
                    <!-- <label for="CNN">CNN</label><br> -->
                    <button class="model-type-button" type="button" name="CNN">
                        <p class="model-type-button-name">
                            CNN
                        </p>
                        <p class="model-type-button-description">
                            multi-layer neural networks to recognize visual patterns directly from pixel images with minimal preprocessing
                        </p>
                    </button>
                </div>
                <!-- <br>
                <br>
                <a href="/">cancel </a>
                <br>
                <a href="/upload">next </a> -->
            </div>
            <div class="footer-nav">
                <a href="/">
                    <button class="nav-button">
                        Cancel
                    </button>
                </a>
                <a href="/upload">
                    <button class="nav-button next-button">
                        Next
                    </button>
                </a>
            </div>
        </div>
    <script>
        function onClickHandler(event) {
            // alert("yo")
            var elems = document.querySelectorAll(".model-type-button");
    
            [].forEach.call(elems, function(el) {
                el.classList.remove("model-type-button-selected");
            });

            event.target.parentNode.classList.add("model-type-button-selected")
        }

    </script>
    </body>
</html>
